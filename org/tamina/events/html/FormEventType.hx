package org.tamina.events.html;


/**
 * Forms API Event
 * <br>
 * DOM Events are sent to notify code of interesting things that have taken place.
 * @class FormEventType
 * @static
 */
class FormEventType {

    /**
     * The submit event is fired when a form is submitted.
     * Note that submit is fired only on the form element, not the button or submit input. (Forms are submitted, not buttons.)
     * @property SUBMIT
     * @type String
     * @static
     * @readOnly
     */
    static public inline var SUBMIT:String='submit';
}
