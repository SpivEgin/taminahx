package org.tamina.events.html;

/**
 * Image API Events
 * @class ImageEventType
 * @static
 */
class ImageEventType {

    /**
     * The load event is fired when progression has begun successful.
     * @property LOAD
     * @type String
     * @static
     * @readOnly
     */
    static public inline var LOAD:String='load';

    /**
     * 	An error occurred.
     * @property ERROR
     * @type String
     * @static
     * @readOnly
     */
    static public inline var ERROR:String='error';
}
